
{% block stylesheets %}
    <style>
        #category{
            display: flex;
            flex-direction: row;
        }
    </style>

{% endblock %}

{% block body %}

    <div class="container-fluid" id="category" >

        <div class="col col-md-3">
            <h3>{{ categoryName }}</h3>
        </div>

        <div class="col col-md-9">
            <div class="container-fluid">

                {% for element in textFields %}
                    {% include '/Profile/profile_fields.html.twig' with element %}
                {% endfor %}

            </div>

        </div>

    </div>

{% endblock %}